{
    "name": "Youtube-AdKiller",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Kills ads on Youtube, without incuring its wrath",
    "browser_action": {
        "default_popup": "src/popup.html",
        "browser_style": true
    },
    "icons": {
        "16": "src/assets/icon16.png",
        "32": "src/assets/icon32.png",
        "64": "src/assets/icon64.png"
    },
    "permissions": ["activeTab"]
}